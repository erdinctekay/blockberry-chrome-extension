{
	"manifest_version": 3,
	"name": "Blockberry - Focus Booster",
	"version": "0.1",
	"description": "Simple, privacy-focused, open-source extension that blocks websites based on a your list. Does not track or collect your data.",
	"permissions": ["tabs", "storage"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "src/popup/popup.html",
		"default_icon": {
			"128": "icons/icon-passive-128.png"
		}
	},
	"options_ui": {
		"page": "src/options/options.html",
		"open_in_tab": true
	},
	"icons": {
		"128": "icons/icon-active-128.png"
	}
}
